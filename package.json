{
  "name": "@click-therapeutics-org/react-native-datepicker",
  "version": "1.7.3",
  "description": "React Native DatePicker component for both Android and iOS, useing DatePickerAndroid, TimePickerAndroid and DatePickerIOS",
  "main": "dist/datepicker.js",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest --coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "eslint .",
    "build": "npx babel ./datepicker.js ./style.js --out-dir dist --copy-files",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clicktherapeutics/react-native-datepicker.git"
  },
  "keywords": [
    "react-native",
    "DatePicker"
  ],
  "author": "xgfe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/clicktherapeutics/react-native-datepicker/issues"
  },
  "homepage": "https://github.com/clicktherapeutics/react-native-datepicker#readme",
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@react-native-community/datetimepicker": "^2.4.0",
    "metro-react-native-babel-preset": "^0.64.0",
    "moment": "^2.22.0"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "^7.2.3",
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "coveralls": "^2.11.9",
    "cz-conventional-changelog": "^1.2.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "inquirer": "^3.0.5",
    "jest": "21.2.1",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "pre-commit": "^1.1.3",
    "react": "16.0.0-beta.5",
    "react-dom": "16.0.0-beta.5",
    "react-native": "0.49.3",
    "react-test-renderer": "16.0.0-beta.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "preset": "react-native",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "tools/"
    ]
  },
  "pre-commit": [
    "lint"
  ]
}
